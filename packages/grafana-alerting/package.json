    "@rtk-query/codegen-openapi": "^2.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "^9.2.0",
    "tsdown": "^0.12.7",
    "type-fest": "^4.40.0",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "@grafana/runtime": ">=11.6 <= 12.x",
